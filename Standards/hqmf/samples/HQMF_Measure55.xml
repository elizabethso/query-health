      </entry>
      <entry>
        <denominatorCriteria>
          <id root="c75181d0-73eb-11de-8a39-0800200c9a66"
            extension="DENOM"/>
          <precondition>
            <atLeastOneTrue>
              <precondition>
                <allTrue>
                  <precondition>
                    <observationReference>
                      <id root="0" extension="HasDiabetes"/>
                    </observationReference>
                  </precondition>
                  <precondition>
                    <atLeastOneTrue>
