                  <id root="0" extension="DiabetesMedAdministered"/>
                </substanceAdministrationReference>
              </precondition>
              <precondition>
                <substanceAdministrationReference moodCode="INT">
                  <id root="0" extension="DiabetesMedIntended"/>
                </substanceAdministrationReference>
              </precondition>
